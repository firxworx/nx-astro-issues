{
  "name": "@nx-astro-issues/source",
  "description": "nx monorepo showcasing some issues with Nx compatibility re Astro 3.0",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nx run-many --target=build --all --parallel=false --output-style=static --verbose --exclude=\"@nx-astro-issues/source\"",
    "build:clean": "nx run-many --target=build --all --parallel=false --output-style=static --verbose --exclude=\"@nx-astro-issues/source\" --skip-nx-cache",
    "test": "NODE_OPTIONS=\"--max-old-space-size=2048\" nx run-many --all --target=test --exclude=\"@nx-astro-issues/source\"",
    "test:watch": "NODE_OPTIONS=\"--max-old-space-size=2048\" nx run-many --all --target=test --watch --exclude=\"@nx-astro-issues/source\"",
    "lint": "NODE_OPTIONS=\"--max-old-space-size=2048\" nx run-many --all --target=lint --exclude=\"*e2e\" --exclude=\"@nx-astro-issues/source\"",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "prettier": "prettier . --write",
    "docker:up": "docker compose up -d && exit 0",
    "docker:stop": "docker compose stop",
    "docker:down": "docker compose down",
    "docker:postgres:up": "docker compose up -d postgres && exit 0",
    "docker:postgres:stop": "docker compose stop pgadmin && exit 0",
    "docker:postgres:bash": "docker exec -it pg-tsrx bash && exit 0",
    "docker:pgadmin:up": "docker compose up -d pgadmin && exit 0",
    "docker:pgadmin:stop": "docker compose stop pgadmin && exit 0",
    "kill:port": "bash -c 'pids=$(lsof -i :$0 | grep LISTEN | awk \"{print \\$2}\"); if [[ -z $pids ]]; then echo \"No process found running on port $0\"; else echo \"Killing: $pids\"; echo $pids | xargs kill; fi'",
    "kill:vscode-server": "ps uxa | grep .vscode-server | awk '{print $2}' | xargs kill",
    "check:node": "which node && node --version",
    "psql:local": "psql -h localhost -U postgres -d postgres"
  },
